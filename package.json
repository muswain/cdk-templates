{
  "name": "cdk-templates",
  "version": "0.1.0",
  "bin": {
    "cdk-templates": "bin/cdk-templates.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "prepare": "husky install",
    "lint": "eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
    "prettier": "prettier \"*.{js,json,yml,md}\" \"packages/**/*\"",
    "format": "npm run prettier -- --write",
    "format:check": "npm run prettier -- --check"
  },
  "devDependencies": {
    "@types/jest": "^29.2.6",
    "@types/node": "18.11.18",
    "aws-cdk": "2.61.1",
    "husky": "^8.0.0",
    "jest": "^29.3.1",
    "lint-staged": "^13.1.0",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "~4.9.4",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.3",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2"
  },
  "dependencies": {
    "aws-cdk-lib": "2.61.1",
    "constructs": "^10.1.226"
  },
  "workspaces": [
    "packages/*"
  ]
}
